<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <List
        items="{
            path: '/Products',
            parameters: { expand: 'Category,Supplier' }
        }">
        <StandardListItem
			title="{Name}"
			description="{Description}"
            
            type="Navigation"
            press=".onProductPress" />
    </List>
</core:FragmentDefinition>